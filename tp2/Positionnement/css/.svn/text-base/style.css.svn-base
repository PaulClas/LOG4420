}

#div6 #div6c {
    position: absolute;
    top: 20px;
    left: 80px;
    background-color: #999;
}

#div6 #div6d {
    position: absolute;
    background-color: #222;
    left: 200px;
}

#div6 #div6e {
    position: absolute;
    left: 240px;
    top: 50px;
    background-color: #666;
    z-index: 1;
}

#div6 #div6f {
    position: absolute;
    left: 280px;
    top: 20px;
    background-color: #999;
}

#div6 #div6g {
    left: 400px;
    position: absolute;
    background-color: #222;
}

#div6 #div6h {
    position: absolute;
    left: 440px;
    top: 50px;
    background-color: #666;
    z-index: -1;
}

#div6 #div6i {
    position: absolute;
    left: 480px;
    top: 20px;
    background-color: #999;
}


.pos-rel-1 {
    position: relative;
    left: 450px;
}

.pos-rel-2 {
    position: relative;
    left: 150px;
}

div div:hover {
    background-color: yellow;
}


.clear {
    clear: both;
}
